{
  "config": {
    "sort-packages": true
  },
  "name": "pe/component-cache-file",
  "description": "Single file cache component with contents fresh tracking",
  "keywords": ["cmf", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pavlenko Eugeniy",
      "email": "pavlenko.obs@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/filesystem": "^3.2",
    "symfony/polyfill-php70": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "PE\\Component\\CacheFile\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PETest\\Component\\CacheFile\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
